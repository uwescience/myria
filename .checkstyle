<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="GWT Checkstyle for Myriad" location=".checkstyle-config" type="project" description="GWT Checkstyle for Myriad">
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="GWT Checkstyle for Myriad" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="protobuf/java"/>
    <filter-data value="src/edu/washington/escience/myria/api/encoding"/>
    <filter-data value="third-party-src"/>
    <filter-data value="test"/>
    <filter-data value="speedtest"/>
    <filter-data value="testsuites"/>
    <filter-data value="systemtest"/>
  </filter>
</fileset-config>
